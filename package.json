{
  "name": "originate-scripts",
  "version": "0.0.0-dev",
  "description": "Utilities to support create-originate-app",
  "license": "MIT",
  "author": "Originate, Inc.",
  "contributors": [
    "Jesse Hallett <jesse.hallett@originate.com>",
    "Danny Meyer <danny.meyer@originate.com>"
  ],
  "repository": "https://github.com/github:Originate/originate-scripts",
  "homepage": "https://github.com/Originate/originate-scripts#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "bin": {
    "originate-scripts": "dist/cjs/originate-scripts.js"
  },
  "files": [
    "dist",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "prepublishOnly": "yarn build && yarn test",
    "build": "rimraf dist && yarn tsc",
    "test": "jest --runInBand",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "originate-scripts": "ts-node --project tsconfig-cjs.json src/originate-scripts.ts"
  },
  "devDependencies": {
    "@types/dockerode": "^3.2.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "git-format-staged": "^2.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "dependencies": {
    "@next/env": "^10.0.3",
    "chalk": "^4.1.0",
    "commander": "^6.2.1",
    "dockerode": "^3.2.1"
  }
}
